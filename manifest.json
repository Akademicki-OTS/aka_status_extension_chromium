{
  "manifest_version": 3,
  "name": "Aka 7.6 Legacy Online Status",
  "version": "1.4.9",
  "description": "View online players and Discord status for Akademicki Open Tibia Server (Aka 7.6 Legacy).",
  "host_permissions": [
    "http://game.ots76.org/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16x16.png",
      "32": "icon32x32.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
      "16": "icon16x16.png",
      "32": "icon32x32.png"
  }
}
